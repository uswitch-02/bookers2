<h1>Books#index</h1>
<!--全投稿が一覧になっている画面-->
<% @book.each do |book| %>
<duv>
    <%= link_to book_path(book.id) do %>
        <%= image_tag book.get_image %>
    <% end %>
    <p><%= image_tag book.user.get_profile_image(100,100) %></p>
  <p>ユーザーネーム:<%= book.user.name %></p>
  <p>Title:<%= book.title %></p>
  <p>opinion:<%= book.body %></p>
</div>
<% end %>
