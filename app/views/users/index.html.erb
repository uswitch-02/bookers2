<html class="fontawesome-i2svg-active fontawesome-i2svg-complete">
<body>
  <main>
    <p id="notice"></p>
    <div class="container px-5 px-sm-0">
  <div class="row">
  <div class="col-md-3">
<%= render 'books/list', user: current_user, book: @book %>
</div>
<div class="col-md-8 offset-md-1">
  <h2>Users</h2>
  <table class="table">
    <thead>
      <tr>
        <th>image</th>
        <th>name</th>
      <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td>
          <% @user.each do |user| %>
          <p><%= image_tag user.get_profile_image(100,100) %></p>
          <p><%= user.name %></p>
          <p><%= link_to "show", user_path(user.id)%></p>
              <% end %>
        </td>
      </tr>
    </tbody>
  </table>
</div>
</div>
</div>
</main>
</body>
</html>

